<div class="container center">
    <% var link1 = link.split('.'),
            linkextension = link1[link1.length - 1]; 
        if(linkextension==='pdf'){ %>
        <p>
            <embed  class="activator" name="plugin" 
            src="https://gradbunker.s3.ap-south-1.amazonaws.com/studentUploads/<%=link%>" 
            type="application/pdf" width="100%" height="300px">
        </p>
        <% } else {  %>
            <img src="https://gradbunker.s3.ap-south-1.amazonaws.com/studentUploads/<%=link%>">
        <% } %>
</div>
