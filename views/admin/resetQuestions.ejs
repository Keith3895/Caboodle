<% include ../partials/header.ejs %>
<title>GradBunker - refresh Question Set</title>
<% include ../partials/navbar.ejs %>

<div class="row">
    <div class="col s12 m10 offset-m1 l6 offset-l3">
      <div class="card-panel">
        <h4 class="center">Refresh Questions for college:</h4>
        <div class="row">
          <form class="col s12" action="/test/refreshQuestions" method="POST" id="SignUpForm">
            <div class="row">
              <div class="input-field col m6 s12">
                <select class="browser-default" id="college" name="college">
                  <option value="" disabled selected>Select College</option>
                  <% for(i=0;i<clgs.length;i++){ %>
                    <option><%= clgs[i] %></option>
                  <% } %>
                </select>
                <div class="input-field col s12">
                  <button class="btn waves-effect waves-light right" type="submit" name="action">Add
                    <i class="material-icons right">send</i>
                  </button>
                </div>
            </div>
          </form>
        </div>
      </div>
    </div>
</div>


<%include ../partials/script.ejs%>
<% include ../partials/footer.ejs %>